package project3;

import java.util.*;

public class Palindrom {

	public static void main(String[] args) {

		 int n;
		 Scanner in = new Scanner(System.in);
		 System.out.println("Cititi numarul nr: ");
	     n = in.nextInt();
		 
		 Palindr(n);
		 
		 
	}
	
	public static void Palindr(int n) {
		int nr = 0;
		
		while(n > 0)
		 {
			nr *= 10;
			nr += n%10;
		    n/=10;
		    System.out.println("" + nr);
		 }
	}

}
