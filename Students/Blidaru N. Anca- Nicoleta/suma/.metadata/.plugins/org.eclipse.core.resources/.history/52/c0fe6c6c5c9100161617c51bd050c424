package project1;

import java.util.*;

public class MyProject {
	public static void main (String[] args) {
		
		String nr;
		
		Scanner in = new Scanner(System.in);
		System.out.println("Cititi numarul nr: ");
		nr = in.nextLine();
		System.out.println("Nr este: " + nr);
		
		//int sum = Suma(s,nr);
		//System.out.println("Suma multiplilor de 3 si 5 mai mici decat nr este: " + sum);
	}


	public static int Suma(int s, int nr) {
		s = 0;
		int i;
		for(i = 1; i<=nr; i++)
			 if(i%3 == 0 || i%5 == 0)
				  s += i;
	
		return s;
	}
}

