package project1;

import java.util.*;

public class MyProject {
	public static void main (String[] args) {
		
		//System.out.println("Nr este: " + nr);
		System.out.println("Suma multiplilor de 3 si 5 mai mici decat nr este: ");
		SumaMultipli();
	}


	public static void SumaMultipli() {
		
		String nr;
		Scanner in = new Scanner(System.in);
		System.out.println("Cititi numarul nr: ");
		nr = in.nextLine();
		
		int s = 0, i;
		for(i = 1; i<= nr; i++)
			 if(i%3 == 0 || i%5 == 0)
				  s += i;
	   
		System.out.println("" + s);
	}
}

