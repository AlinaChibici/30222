import java.util.*;

public class Fibonaci {

	public static void main(String[] args) {
		
		//Integer n;
		//Scanner in = new Scanner(System.in);
		//n = in.nextInt();
		int n = 1;
		
		if(n <= 2) System.out.println("" + n);
        
		int a = 1, b = 1, c;
	    for (int i = 0; i < n - 2; i++)
	       {
		     c = a + b;
		     b = a;
		     a = c;
		   }
		   
	    System.out.println("" + a);

	}

}
